{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:write-all",
    "fs:allow-stat",
    "fs:read-all",
    "fs:allow-rename",
    {
      "identifier": "fs:allow-copy-file",
      "allow": [
        { "path": "**" }
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        { "path": "**" }
      ]
    },
    "opener:default"
  ]
}
